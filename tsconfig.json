{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/app/*"],
      "@env": ["./env-config.ts"],
      "@assets/*": ["./assets/*"],
      "@common/*": ["./src/app/common/*"],
      "@app-emitter": ["./src/app/common/emitter/index"],
      "@app-firebase": ["./src/app/common/firebase/index"],
      "@hooks": ["./src/app/common/hooks/index"],
      "@validate/*": ["./src/app/common/zod-validate/*"],
