{
  "expo": {
    "name": "hanvietairApp",
    "slug": "hanvietairApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "platforms": [
      "ios",
      "android"
    ],
    "scheme": "hanvietairApp",
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.hanvietairApp"
    },
    "android": {
      "package": "com.hanvietairapp"
    },
    "plugins": [
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      "react-native-keys",
      "expo-font"
    ]
  }
}
